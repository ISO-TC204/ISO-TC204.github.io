#################################################################
#    Object Properties
#################################################################

###  https://isotc204.org/ontologies/its/location#LocationObjectProperty
:LocationObjectProperty rdf:type owl:ObjectProperty .


###  https://isotc204.org/ontologies/its/location#areaLocation
:areaLocation rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :LocationObjectProperty ;
              rdfs:range [ rdf:type owl:Restriction ;
                           owl:onProperty :areaLocation ;
                           owl:allValuesFrom :AreaLocation
                         ] ;
              terms:description "location defined by an area covering the surface (or near-surface) of the Earth" .


###  https://isotc204.org/ontologies/its/location#areasAtWhichApplicable
:areasAtWhichApplicable rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :LocationObjectProperty .


###  https://isotc204.org/ontologies/its/location#coordinatesForDisplay
:coordinatesForDisplay rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :LocationObjectProperty .


###  https://isotc204.org/ontologies/its/location#destination
:destination rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :LocationObjectProperty ;
             rdfs:range [ rdf:type owl:Restriction ;
                          owl:onProperty :destination ;
                          owl:allValuesFrom :Destination
                        ] .


###  https://isotc204.org/ontologies/its/location#directionPurpose
:directionPurpose rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :LocationObjectProperty .


###  https://isotc204.org/ontologies/its/location#externalLocationReference
:externalLocationReference rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :LocationObjectProperty ;
                           rdfs:domain [ rdf:type owl:Restriction ;
                                         owl:onProperty :externalLocationReference ;
                                         owl:allValuesFrom code:Code
                                       ] .


###  https://isotc204.org/ontologies/its/location#geographicDescriptor
